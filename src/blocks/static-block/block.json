{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "bhe/static-block",
	"version": "0.1.0",
	"title": "Static block",
	"category": "text",
	"icon": "flag",
	"description": "",
	"attributes": {
		"content": {
			"type": "string",
			"source": "text",
			"selector": ".content",
			"frontend": true
		}
	},
	"providesContext": {
		"bhe/content": "content"
	},
	"supports": {
		"color": {
			"text": true
		},
		"html": true
	},
	"textdomain": "static-block",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
